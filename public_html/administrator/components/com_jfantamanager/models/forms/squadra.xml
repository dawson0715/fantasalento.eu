<form>
    <fieldset>
        <field
            name="id"
            type="text"
            class="readonly"
            label="COM_JFANTAMANAGER_SQUADRAS_HEADING_ID"
            description ="COM_JFANTAMANAGER_SQUADRA_ID_DESC"
            size="10"
            default="0"
            readonly="true"
        />
        <field
                name="nome"
                type="text"
                label="COM_JFANTAMANAGER_SQUADRAS_HEADING_NOME"
                description="COM_JFANTAMANAGER_SQUADRA_NOME_DESC"
                size="40"
                class="inputbox"
                default=""
        />
        <field
                name="logo"
                type="media"
                label="COM_JFANTAMANAGER_SQUADRAS_HEADING_LOGO"
                description="COM_JFANTAMANAGER_SQUADRA_LOGO_DESC"
                directory="com_jfantamanager"
        />
        <field
                name="cambi"
                type="text"
                label="COM_JFANTAMANAGER_SQUADRAS_HEADING_CAMBI"
                description="COM_JFANTAMANAGER_SQUADRA_CAMBI_DESC"
                size="40"
                class="inputbox"
                default=""
        />
        <field
                name="pagato"
                type="text"
                label="COM_JFANTAMANAGER_SQUADRAS_HEADING_PAGATO"
                description="COM_JFANTAMANAGER_SQUADRA_PAGATO_DESC"
                size="40"
                class="inputbox"
                default=""
        />
        <field
                name="bilancio"
                type="text"
                label="COM_JFANTAMANAGER_SQUADRAS_HEADING_BILANCIO"
                description="COM_JFANTAMANAGER_SQUADRA_BILANCIO_DESC"
                size="40"
                class="inputbox"
                default=""
        />
        <field name="created_by" type="user"
                label="COM_JFANTAMANAGER_SQUADRAS_HEADING_USER"
                description="COM_JFANTAMANAGER_SQUADRAS_USER_DESC"
        />
        <field
                name="lega"
                type="leghe"
                extension="com_jfantamanager"
                class="inputbox"
                default=""
                label="COM_JFANTAMANAGER_SQUADRA_LEGA_LABEL"
                description="COM_JFANTAMANAGER_SQUADRA_LEGA_DESC"
                required="true"
        ></field>
        <field
                name="permesso"
                type="list"
                size="1"
                label="COM_JFANTAMANAGER_SQUADRA_PERMESSO_LABEL"
                description="COM_JFANTAMANAGER_SQUADRA_PERMESSO_DESC"
                default="0">
                <option value="0">Nessun permesso</option>
                <option value="1">Permesso</option>
                <option value="2">Bloaccato</option>
        </field>
    </fieldset>
    <fields name="params">
        <fieldset name="jbasic" label="COM_WEBLINKS_FIELDSET_OPTIONS">
            
        </fieldset>
    </fields>
</form>
